{
  "name": "listvalueimagegenerator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://stevanfreeborn.github.io/ListValueImageGenerator/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist --repo https://github.com/StevanFreeborn/ListValueImageGenerator.git",
    "test": "vitest",
    "coverage": "vitest run --coverage"
